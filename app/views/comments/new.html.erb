<%# <h3>Make a Comment About <%= @funding_project.title %>!</h4> %>

<%# <%= binding.pry %> %>
<%= form_for @comment, url: funding_project_comments_path do |f| %>
<%= f.hidden_field :funding_project_id %>
<%= f.label :title %>
<%= f.text_field :title %><br>
<%= f.label "What do you think?" %>
<%= f.text_area :content %><br>
<%= f.submit "Submit"%>
<% end %>
